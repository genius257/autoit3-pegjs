{
  "name": "autoit3-pegjs",
  "description": "A PEG.js parser for the AutoIt3 language",
  "main": "out/autoit3.js",
  "scripts": {
    "test": "vitest",
    "build": "peggy -o out/autoit3.js src/autoit3.pegjs --cache --dts --return-types {\\\"Start\\\":\\\"AutoIt3.Start\\\"} && tsx build.ts",
    "install": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/genius257/autoit3-pegjs.git"
  },
  "keywords": [
    "autoit3"
  ],
  "author": "genius257",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/genius257/autoit3-pegjs/issues"
  },
  "homepage": "https://github.com/genius257/autoit3-pegjs#readme",
  "dependencies": {
    "peggy": "^4.2.0",
    "peggy-to-ts": "github:genius257/peggy-to-ts#semver:^1.0.4",
    "tsx": "^4.19.2"
  },
  "devDependencies": {
    "@types/node": "^18.15.3",
    "vitest": "^1.3.1"
  },
  "type": "module"
}
